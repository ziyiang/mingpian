<template name="cardType1">
    <view style="background:white;padding:15rpx 0rpx;">
        <view class="card-index-sec br-10">
            <image mode="aspectFill" src="{{tmpCardData.avatar?tmpCardData.avatar:tmpCardData.default}}"></image>
            <view class="user-card-info rel">
                <image class="circle-img abs" src="/longbing_card/resource/images/circle.png"></image>
                <image class="circle abs" mode="aspectFill" src="{{tmpCardData.logo?tmpCardData.logo:tmpCardData.default}}"></image>
                <view class="tc name ellipsis">
                    {{tmpCardData.name?tmpCardData.name:'暂未填写'}}
                </view>
                <view class="tc ellipsis">
                    {{tmpCardData.job_name?tmpCardData.job_name:'暂未填写'}}
                </view>
                <view class="tc ellipsis">
                    {{tmpCardData.company_name?tmpCardData.company_name:'暂未填写'}}
                </view>
            </view>
        </view>
    </view>
</template>
