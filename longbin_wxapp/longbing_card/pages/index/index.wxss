page {
    background: #f4f4f8;
}

view,button {
    display: block;
    overflow: initial;
}

.animatoins {
    width: 94%;
    height: 118rpx;
    background: #fefefd;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: fixed;
    top: 10rpx;
    left: 3%;
    z-index: 9999999;
    color: #000;
    font-size: 28rpx;
    transition: transform 1s;
    transform: translateY(-140rpx);
    box-shadow: 0 3rpx 12rpx rgba(0,0,0,.1);
}

.animatoins text {
    width: 500rpx;
    position: relative;
    top: -5rpx;
}

.aa {
    transform: translateY(0rpx);
}

.animatoins_top {
    display: flex;
    flex-direction: column;
    padding: 10rpx 0 0 0;
    flex: 1;
}

.animatoins_content {
    color: #666;
    font-size: 22rpx;
}

.animatoins image {
    width: 64rpx;
    height: 64rpx;
    padding: 0rpx 10rpx 0rpx 30rpx;
    margin: 0rpx;
}

.animatoins_top em {
    top: 27rpx;
    right: 5%;
}

.scroll-view-navTop-sec {
    height: 90rpx;
    line-height: 90rpx;
}

.scroll-item-de {
    height: 80rpx;
    line-height: 80rpx;
}